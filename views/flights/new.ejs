<%- include('../partials/header') %>


<form id="new-form" action="/flights" method="POST">
    <label>Airline:</label><br>
    <input type="text" name="airline"><br>
    <label>Flight Number:</label><br>
    <input type="number" name="flightNo"><br>
    <label>Departs:</label><br>
    <input type="date" name="departs"><br>
  
  <select name="airport">
    <option value="1">ATX</option>
    <option value="2">SAN</option>
    <option value="3">SEA</option>
    <option value="4">LAX</option>
    <option value="5">DAL</option>
  </select>
  <input type="submit" value="Add Flight">
</form>
  <%- include('../partials/footer') %>